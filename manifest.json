{
	"name": "Jira estimate summarizer",
	"version": "1.0",
	"description": "Adds UI to Jira Cloud next-gen projects that helps working with estimates",
	"author": "Henrik Gunnarsson",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["https://*.atlassian.net/browse/*"],
			"css": ["styles.css"],
			"js": ["script.js"]
		}
	],
	"permissions" : [
		"tabs",
		"https://*.atlassian.net/browse/*"
	],
	"manifest_version": 2
}
