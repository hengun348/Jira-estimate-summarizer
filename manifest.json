{
	"name": "Jira estimate summarizer",
	"version": "1.0",
	"description": "Shows estimate summaries and percentage done (base on estimates) in the epics details in Jira Cloud, next-gen projects",
	"author": "Henrik Gunnarsson",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["https://*.atlassian.net/browse/*"],
			"css": ["styles.css"],
			"js": ["script.js"]
		}
	],
	"permissions" : [
		"tabs",
		"https://*.atlassian.net/browse/*"
	],
	"icons" : {
		"16" : "icon-16x16.png",
		"32" : "icon-32x32.png",
		"192" : "icon-192x192.png"
	},
	"manifest_version": 2
}
